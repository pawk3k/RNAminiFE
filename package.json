{
  "name": "rna-renifier",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "compress": "gzipper c --gzip-level 9 -v --threshold 256 --remove-larger ./build",
    "custom-start": "node server.js",
    "dev": "next dev -p 4010",
    "build": "next build ",
    "start": "next start -p 5000",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx --cache",
    "lint:ci": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "serve:lhci": "npm run start",
    "prepare:hooks": "husky install .husky",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test dev http://localhost:4010 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:4010 cy:open"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@reach/dialog": "^0.16.2",
    "@reach/tabs": "^0.16.4",
    "@tanem/react-nprogress": "^3.0.82",
    "axios": "^0.24.0",
    "gzipper": "^6.2.1",
    "molstar": "^3.0.0-dev.4",
    "molstar-react": "^0.3.0",
    "next": "12.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-svgr": "^0.0.2",
    "parse-pdb": "^1.0.0",
    "patch-package": "^6.4.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.5",
    "react-toastify": "^8.1.0",
    "react-use": "^17.3.2",
    "tailwindcss-textshadow": "^2.1.3"
  },
  "devDependencies": {
    "@masterborn/eslint-config": "0.0.13",
    "@testing-library/cypress": "^8.0.2",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "autoprefixer": "10.4.0",
    "cypress": "^9.2.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.1.2",
    "postcss": "8.4.4",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "3.0.0",
    "typescript": "4.4.4"
  }
}
